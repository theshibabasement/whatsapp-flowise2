#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules/@open-wa/wa-automate/bin/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules/@open-wa/wa-automate/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules/@open-wa/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules/@open-wa/wa-automate/bin/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules/@open-wa/wa-automate/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules/@open-wa/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/@open-wa+wa-automate@4.67.0_lodash@4.17.21_prom-client@14.2.0_typescript@5.3.3_webpack@5.90.0/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@open-wa/wa-automate/bin/server.js" "$@"
else
  exec node  "$basedir/../@open-wa/wa-automate/bin/server.js" "$@"
fi
