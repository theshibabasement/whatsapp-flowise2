#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/cfonts@2.10.1/node_modules/cfonts/bin/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/cfonts@2.10.1/node_modules/cfonts/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/cfonts@2.10.1/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/cfonts@2.10.1/node_modules/cfonts/bin/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/cfonts@2.10.1/node_modules/cfonts/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/cfonts@2.10.1/node_modules:/mnt/e/github/whatsapp-flowise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../cfonts@2.10.1/node_modules/cfonts/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../../cfonts@2.10.1/node_modules/cfonts/bin/index.js" "$@"
fi
